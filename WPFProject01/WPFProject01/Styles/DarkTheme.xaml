<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="BackgroundColor">#FF2D2D30</Color>
    <Color x:Key="ForegroundColor">White</Color>

    <SolidColorBrush x:Key="AppBackground" Color="{StaticResource BackgroundColor}" />
    <SolidColorBrush x:Key="AppForeground" Color="{StaticResource ForegroundColor}" />
</ResourceDictionary>